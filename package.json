{
  "name": "deps",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Francois Wouts",
  "license": "MIT",
  "scripts": {
    "start": "yarn serve",
    "generate": "ts-node ./src/generate/main.ts",
    "generate:live": "nodemon -e ts --watch src/generate/ --exec yarn generate",
    "viz:build": "webpack",
    "viz:livebuild": "nodemon -e ts --watch src/viz/ --exec yarn viz:build",
    "serve": "yarn generate && yarn viz:build && concurrently \"http-server dist/viz/ -a localhost -p 1234\" \"open http://localhost:1234/\""
  },
  "dependencies": {
    "graphdracula": "^1.1.2",
    "nofiles": "^1.0.4",
    "raphael": "^2.2.7",
    "typescript": "^2.4.2"
  },
  "devDependencies": {
    "@types/node": "^8.0.25",
    "awesome-typescript-loader": "^3.2.3",
    "concurrently": "^3.5.0",
    "http-server": "^0.10.0",
    "nodemon": "^1.11.0",
    "source-map-loader": "^0.2.1",
    "ts-node": "^3.3.0",
    "webpack": "^3.5.5"
  }
}
